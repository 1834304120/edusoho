<div>
  <form class="form-inline js-search-form" novalidate>
    <div class="form-group">
      <input type="text" id="keyword" name="keyword" data-role="search-conditions" class="form-control" value="{{ app.request.query.get('keyword') }}" placeholder="{{'course.question_manage.filter.keywords'|trans}}">
    </div>
    <div class="form-group">
      <button class="cd-btn cd-btn-primary js-search-btn">{{'form.btn.search'|trans}}</button>
    </div>

    <div class="form-group pull-right">
        <a href="" class="cd-btn cd-btn-primary pull-right mls">
          {{ 'course.testpaper_manage.import_btn'|trans }}
        </a>
        <a class="cd-btn cd-btn-primary pull-right mls js-import-btn" data-url="" data-toggle="modal" data-target="#modal">
          {{ 'course.testpaper_manage.create_btn'|trans }}
        </a>
      </div>
  </form>

  <div role="courseset-manage-question">
    <div id="quiz-table-container" class="js-testpaper-html" data-url="{{ path('question_bank_manage_testpaper_show_ajax', {id: questionBank.id}) }}">
      {% include 'question-bank/testpaper/testpaper-list-tr.html.twig' %}
    </div>
  </div>
</div>