<div class="material-common-table">
  <div class="table-responsive">
    <table class="table table-striped table-hover mtl" id="materials-table" data-url="{{ path('admin_cloud_file_render_table')}}">
      {# <thead>
        <tr>
          <th width="40%">{{ 'file'|trans }}</th>
          <th width="" class="text-center">{{ 'type'|trans }}</th>
          <th width="" class="text-center">{{ 'size'|trans }}</th>
          <th width="" class="text-center">{{ 'cloud_file.upload_time'|trans }}</th>
          {% if pageType == 'attachment'%}
            <th width="" class="text-center">{{ 'cloud_file.application'|trans }}</th>
          {% endif %}
