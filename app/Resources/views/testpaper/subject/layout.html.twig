{% extends 'layout.html.twig' %}

{% do script(['app/js/subject/index.js']) %}

{% block content %}
  <div class="row">
    <div class="col-sm-3">
      <div class="subject-data subject-rect js-subject-data">
        <div class="subject-rect__title">题目统计
          {% block subtitle %}{% endblock %}
        </div>
        {% include 'testpaper/subject/subject-num.html.twig' %}
      </div>
      {% include 'testpaper/subject/subject-list.html.twig' %}
    </div>
    <div class="col-sm-9">
      {% block testpaper %}{% endblock %}
      {% include 'testpaper/subject/choose-subject.html.twig' %}
      {% include 'testpaper/subject/answer-subject.html.twig' %}
      {% include 'testpaper/subject/material-subject.html.twig' %}
      <div class="text-center cd-mt24">
        <button class="cd-btn cd-btn-primary subject-button" type="button">完成导入</button>
      </div>
    </div>
  </div>
  {% block question_difficulty_modal %}
    {% include 'testpaper/subject/question-difficulty-modal.html.twig' %}
  {% endblock %}
{% endblock %}