{% extends 'testpaper/subject/layout.html.twig' %}

{% set totolNum =  19 %}
{% set totolScore =  200 %}

{% block subtitle %}
  <div class="subject-data__subtitle mts clearfix">
    <span>共{{ totolNum }}道题</span>
    <span class="pull-right">总分{{ totolScore }}分</span>
  </div>
{% endblock %}

{% block testpaper %}
  <div class="subject-title"><input name="testpaperTitle" value="试卷名称" placeholder="此处可编辑试卷名称，最多输入50个字符"></div>
{% endblock %}